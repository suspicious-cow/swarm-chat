# PostgreSQL
POSTGRES_USER=swarmchat
POSTGRES_PASSWORD=swarmchat_dev_password
POSTGRES_DB=swarmchat
DATABASE_URL=postgresql+asyncpg://swarmchat:swarmchat_dev_password@postgres:5432/swarmchat

# Redis
REDIS_URL=redis://redis:6379/0

# LLM Configuration â€” swap providers by changing these values
LLM_PROVIDER=gemini
LLM_API_KEY=your-api-key-here
LLM_MODEL=gemini-3-flash-preview
#
# Example configs for other providers (uncomment one block):
#
# --- Gemini (default) ---
# LLM_PROVIDER=gemini
# LLM_MODEL=gemini-3-flash-preview
#
# --- OpenAI ---
# LLM_PROVIDER=openai
# LLM_MODEL=gpt-5.2
#
# --- Anthropic ---
# LLM_PROVIDER=anthropic
# LLM_MODEL=claude-haiku-4-5-20251001
#
# --- Mistral ---
# LLM_PROVIDER=mistral
# LLM_MODEL=mistral-medium-3-1-25-08
#
# --- DeepSeek (openai-compatible) ---
# LLM_PROVIDER=openai-compatible
# LLM_BASE_URL=https://api.deepseek.com
# LLM_MODEL=deepseek-chat
#
# --- Local (Ollama / vLLM) ---
# LLM_PROVIDER=openai-compatible
# LLM_BASE_URL=http://localhost:11434/v1
# LLM_MODEL=llama3

# Backend
SECRET_KEY=dev-secret-key-change-in-production
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:80

# Engine settings
SUBGROUP_SIZE=5
CME_INTERVAL_SECONDS=20
SURROGATE_INTERVAL_SECONDS=30
